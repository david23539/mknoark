{
  "name": "mk-noark",
  "version": "1.0.0-demo",
  "repository": {
    "url": "https://github.com/david23539/mknoark.git",
    "type": "git"
  },
  "license": "propietary",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-hmr": "ng serve --hmr",
    "start-remote": "ng serve --host 0.0.0.0 --port 4202",
    "start-remote-prod": "ng serve --prod --host 0.0.0.0",
    "build": "ng build",
    "test": "ng test --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --write \"src/**/*.ts\"",
    "packagr": "ng-packagr -p ng-package.json && npm run compile-sass",
    "compile-sass": "sass src/styles.scss:dist/styles.min.css --style=compressed --no-source-map=none && npm run copy-images && npm run run-script:replace && npm run create-folder-fonts && npm run copy-fonts",
    "run-script:replace": "chmod +x \"./script/replace.sh\" && \"./script/replace.sh\"",
    "create-folder-fonts": "mkdir \"./dist/fonts\"",
    "copy-fonts": "cp -R \"./src/assets/icons\" \"./dist/fonts\"  && cp -R \"./src/assets/fonts\" \"./dist/fonts\"",
    "copy-images": "cp -R \"./src/assets/images\" \"./dist/images\"",
    "doc": "npx compodoc -p tsconfig.json --language es-ES --disableCoverage"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~11.0.3",
    "@angular/common": "~11.0.3",
    "@angular/compiler": "~11.0.3",
    "@angular/core": "~11.0.3",
    "@angular/forms": "~11.0.3",
    "@angular/platform-browser": "~11.0.3",
    "@angular/platform-browser-dynamic": "~11.0.3",
    "@angular/router": "~11.0.3",
    "@briebug/jest-schematic": "^3.1.0",
    "core-js": "^3.2.1",
    "node-sass": "^4.14.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "latest",
    "@angular-devkit/build-angular": "^0.1100.3",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "^11.2.7",
    "@angular/compiler-cli": "^11.0.3",
    "@angular/language-service": "~11.0.3",
    "@compodoc/compodoc": "^1.1.15",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/jest": "latest",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "latest",
    "ng-packagr": "^11.0.3",
    "prettier": "1.19.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "^4.0.5"
  }
}
